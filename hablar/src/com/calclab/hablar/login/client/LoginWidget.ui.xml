<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>

	</ui:style>
	<g:DockLayoutPanel addStyleNames="hablar-Login hablar-Page hablar-lightBackground" unit="EM">
		<g:north size="9">
			<g:HTMLPanel addStyleNames="form">
				<p>
					<label ui:field="userLabel">User:</label>
					<g:TextBox  ui:field="user" />
				</p>
				<p>
					<label ui:field="passwordLabel">Password:</label>
					<g:PasswordTextBox ui:field="password" />
				</p>
				<div addStyleNames="actions">
					<g:Button ui:field="button">Login</g:Button>
				</div>
			</g:HTMLPanel>
		</g:north>
		<g:center>
			<g:FlowPanel ui:field="output" addStyleNames="output hablar-clearBackground"></g:FlowPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder>