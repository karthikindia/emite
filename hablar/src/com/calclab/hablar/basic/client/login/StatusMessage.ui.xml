<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="LoginPage.css" />
		
	<ui:style field="icons">
		@sprite .info {
			gwt-image: 'info';
		}
	</ui:style>
	<ui:image field="info" src="info.png"></ui:image>
	<g:HTMLPanel styleName="{style.message}">
		<span class='{style.icon} {icons.info}' ui:field="icon"></span>
		<g:Label styleName="{style.body}" ui:field="body"></g:Label>
	</g:HTMLPanel>
</ui:UiBinder> 