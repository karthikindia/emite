<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>

	</ui:style>
	<g:HTMLPanel addStyleNames="hablar-EditBuddyWidget">
		<div class="form">
			<h2>
				<span ui:field='title' />
			</h2>
			<p>
				<label ui:field="changeFromLabel">Change from:</label>
				<g:Label ui:field="oldNickName">oldNickName</g:Label>
			</p>
			<p>
				<label ui:field="changeToLabel">To:</label>
				<g:TextBox ui:field="nickName">newNickName</g:TextBox>
				<g:Label ui:field="nickNameError" addStyleNames="error"></g:Label>
			</p>
			<div class="actions hablar-topDefaultBordered">
				<g:Button ui:field="save">Change</g:Button>
				<g:Button ui:field="cancel">Cancel</g:Button>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>