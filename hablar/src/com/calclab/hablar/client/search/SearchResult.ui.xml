<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style src="../Hablar.css" field="hablar" />
		
	<ui:style src="SearchPage.css" />
		
	<ui:style field="icons">
		@sprite .userAdd {
			gwt-image: 'buddy';
		}
	</ui:style>

	<ui:image field="buddy" src="buddy.gif"></ui:image>
	<g:HTMLPanel styleName="{style.result}">
		<span class="{hablar.icon} {icons.userAdd}"></span>
		<g:Label ui:field="name">Result</g:Label>
	</g:HTMLPanel>
</ui:UiBinder> 