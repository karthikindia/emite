<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style type="com.calclab.hablar.client.ui.icons.Icons.Styles">
		@sprite .buddyIcon {
			gwt-image: 'buddy';
		}
		
		@sprite .buddyIconOn {
			gwt-image: 'buddyOn';
		}
		
		@sprite .buddyIconOff {
			gwt-image: 'buddyOff';
		}
		
		@sprite .buddyIconDnd {
			gwt-image: 'buddyDnd';
		}
		
		@sprite .buddyIconWait {
			gwt-image: 'buddyWait';
		}
		
		@sprite .rosterIcon {
			gwt-image: 'rosterIcon';
		}
	</ui:style>

	<ui:image field="rosterIcon" src="roster.png"></ui:image>
	<ui:image field="buddy" src="buddy-small.png"></ui:image>
	<ui:image field="buddyOn" src="buddy-small-on.png"></ui:image>
	<ui:image field="buddyOff" src="buddy-small-off.png"></ui:image>
	<ui:image field="buddyDnd" src="buddy-small-dnd.png"></ui:image>
	<ui:image field="buddyWait" src="buddy-small-wait.png"></ui:image>

	<g:SimplePanel></g:SimplePanel>
</ui:UiBinder> 