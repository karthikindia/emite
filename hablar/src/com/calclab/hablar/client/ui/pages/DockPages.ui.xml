<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:pages="urn:import:com.calclab.hablar.client.ui.pages">
	<ui:style>
		.dock {
			
		}
		
		.container {
			width: 100%;
			height: 100%;
			background-color: red;
			border: 1px solid #bbbbbb;
		}
	</ui:style>
	<g:DockLayoutPanel unit="PX" styleName="{style.dock}">
		<g:west size='200'>
			<g:DockLayoutPanel ui:field="west" unit=""
				styleName="{style.container}">
			</g:DockLayoutPanel>
		</g:west>
		<g:center>
			<pages:TabPages ui:field="pages"></pages:TabPages>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 