<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel styleName="hablar-VCardWidget">
		<div class="form">
			<h2>
				<span ui:field='title' />
			</h2>
			<p>
				<label>Name:</label>
				<g:TextBox ui:field="name"></g:TextBox>
			</p>
            <p>
                <label>Nickname:</label>
                <g:TextBox ui:field="nickName"></g:TextBox>
            </p>
            <p>
                <label>Family name:</label>
                <g:TextBox ui:field="familyName"></g:TextBox>
            </p>
			<p>
				<label>Middle name:</label>
				<g:TextBox ui:field="middleName"></g:TextBox>
			</p>
            <p>
                <label>Given name:</label>
                <g:TextBox ui:field="givenName"></g:TextBox>
            </p>
			<p>
				<label>Organization:</label>
				<g:TextBox ui:field="organizationName"></g:TextBox>
			</p>
            <p>
                <label>Homepage:</label>
                <g:TextBox ui:field="homepage"></g:TextBox>
            </p>
			<p>
				<label>Email:</label>
				<g:TextBox ui:field="email"></g:TextBox>
			</p>
			<div class="actions">
				<g:Button ui:field="accept">Accept</g:Button>
				<g:Button ui:field="cancel">Cancel</g:Button>
			</div>
		</div>
	</g:HTMLPanel>

</ui:UiBinder> 