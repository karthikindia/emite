<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel styleName="hablar-VCardWidget">
		<div class="form">
			<p>
				<label>Nickname:</label>
				<g:TextBox ui:field="nickName"></g:TextBox>
			</p>
			<p>
				<label>First name:</label>
				<g:TextBox ui:field="firstName"></g:TextBox>
			</p>
			<p>
				<label>Middle name:</label>
				<g:TextBox ui:field="middleName"></g:TextBox>
			</p>
			<p>
				<label>Surname:</label>
				<g:TextBox ui:field="surnameName"></g:TextBox>
			</p>
			<p>
				<label>Organization:</label>
				<g:TextBox ui:field="organizationName"></g:TextBox>
			</p>
            <p>
                <label>Email:</label>
                <g:TextBox ui:field="email"></g:TextBox>
            </p>

		</div>
	</g:HTMLPanel>

</ui:UiBinder> 